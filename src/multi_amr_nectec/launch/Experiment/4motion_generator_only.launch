<launch>
<node pkg="multi_amr_nectec" type="motion_generator" name="motion_generator" output="screen" >
 <param name ="team_size" value = "2" />
<param name ="v_linear_threshold" value = "0.1" />
<param name ="v_angular_threshold" value = "0.2" />
<param name="v_linear_cutoff" value="0.05"/>
<param name="v_angular_cutoff" value="0.02"/>
<param name ="robot_mass" value = "40" />
<param name ="robot_radious" value = "0.5" />
<param name ="robot_inertia" value = "0.77" />
  <param name ="delta_time" value = "1" />
</node>
</launch>