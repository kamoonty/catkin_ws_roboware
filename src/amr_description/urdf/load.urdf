<robot name="simple_box">
  <link name="my_box">
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="1.0" />
      <inertia  ixx="1.66" ixy="0.0"  ixz="0.0"  iyy="1.66"  iyz="0.0"  izz="1.66" />
    </inertial>

    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.1 1 0.1" />
      </geometry>
    </visual>

    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.1 1 0.1" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="load_transport">
    <material>Gazebo/Green</material>
  </gazebo>
</robot>
