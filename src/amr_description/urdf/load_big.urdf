<?xml version="1.0"?>
<robot name="simple_box" xmlns:xacro="http://ros.org/wiki/xacro">
  
  <link name="my_box">
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="550.0" />
      <inertia  ixx="100" ixy="0.0"  ixz="0.0"  iyy="100"  iyz="0.0"  izz="100" />
    </inertial>

    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="5 5 0.2" />
      </geometry>
    </visual>

    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="5 5 0.2" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="my_box">
   <material>Gazebo/Yellow</material>
    <mu1>9000</mu1>
    <mu2>9000</mu2>
    <fdir1>1 0 0</fdir1>
  </gazebo>
</robot>
