<?xml version="1.0"?>
<robot name="simple_box" xmlns:xacro="http://ros.org/wiki/xacro">
  
  <link name="my_box">
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="300.0" />
      <inertia  ixx="1.66" ixy="0.0"  ixz="0.0"  iyy="1.66"  iyz="0.0"  izz="1.66" />
    </inertial>

    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="4 4 0.1" />
      </geometry>
    </visual>

    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="4 4 0.1" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="my_box">
   <material>Gazebo/Yellow</material>
    <mu1>1000</mu1>
    <mu2>1000</mu2>
    <fdir1>1 0 0</fdir1>
  </gazebo>
</robot>
